{
  "name": "cap-vv-powershell",
  "version": "0.0.1",
  "description": "Plugin Powershell Electron",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "build-electron": "rimraf ./electron/dist && cd ./electron && tsc && rollup --config rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "Herv√© de CHAVIGNY",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "^2.2.0"
  },
  "devDependencies": {
    "@capacitor/android": "latest",
    "@capacitor/ios": "latest",
    "rimraf": "^3.0.0",
    "rollup": "^2.18.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "typescript": "^3.2.4"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapVvPowershell.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    },
    "electron": {
      "src": "electron/dist/plugin.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vevedh/cap-vv-powershell.git"
  },
  "bugs": {
    "url": "https://github.com/vevedh/cap-vv-powershell.git/issues"
  }
}
