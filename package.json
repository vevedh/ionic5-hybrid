{
  "name": "ionic5hybrid",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "electron:prod": "ng run app:build --base-href=./ && @powershell copy  -recurse -force www electron/server/ && npx cap update @capacitor-community/electron && npx cap copy && @powershell copy capacitor.config.prod.json electron/capacitor.config.json && npx cap open @capacitor-community/electron",
    "electron:win": "ng run app:build --base-href=./ && @powershell copy  -recurse -force www electron/server/ && npx cap update @capacitor-community/electron && npx cap copy && @powershell copy capacitor.config.prod.json electron/capacitor.config.json && cd electron && npm run electron:build-windows",
    "electron:start": "npx cap open @capacitor-community/electron",
    "electron:dev": "npm-run-all -p build start start:dev",
    "start:dev": "wait-on http://localhost:8100 && npm run electron:start",
    "start": "@powershell $env:NODE_ENV='production' && npx cap update  @capacitor-community/electron && @powershell copy capacitor.config.dev.json electron/capacitor.config.json && @powershell copy capacitor.config.dev.json capacitor.config.json && ionic serve --no-open",
    "build": "ng run app:build --base-href=./",
    "build:linux": "ng run app:build --base-href=./ && cp -rf www electron/server/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^11.0.1",
    "@angular/common": "~11.0.2",
    "@angular/core": "^11.0.2",
    "@angular/forms": "~11.0.2",
    "@angular/platform-browser": "~11.0.2",
    "@angular/platform-browser-dynamic": "~11.0.2",
    "@angular/router": "~11.0.2",
    "@capacitor-community/electron": "^1.3.2",
    "@capacitor/core": "^2.4.3",
    "@feathersjs/authentication-client": "^4.5.10",
    "@feathersjs/feathers": "^4.5.10",
    "@feathersjs/socketio-client": "^4.5.10",
    "@ionic-native/core": "^5.0.7",
    "@ionic-native/splash-screen": "^5.30.0",
    "@ionic-native/status-bar": "^5.30.0",
    "@ionic/angular": "^5.5.1",
    "@ionic/storage": "^2.3.1",
    "@ngx-formly/core": "^5.10.6",
    "@ngx-formly/ionic": "^5.10.6",
    "@ngx-formly/schematics": "^5.10.6",
    "cap-powershell": "^2.1.1",
    "capacitor-plugin-nodemailer": "^1.0.2",
    "ng2-file-upload": "^1.4.0",
    "rxjs": "~6.6.3",
    "spawnd": "^4.4.0",
    "tslib": "^2.0.0",
    "webdatarocks": "^1.3.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.2",
    "@angular/cli": "^11.0.2",
    "@angular/compiler": "~11.0.2",
    "@angular/compiler-cli": "^11.0.2",
    "@angular/language-service": "~11.0.2",
    "@capacitor/cli": "^2.4.3",
    "@ionic/angular-toolkit": "^3.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.1",
    "electron": "^11.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ngx-electron": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5",
    "wait-on": "^5.2.0"
  },
  "description": "An Ionic project"
}
